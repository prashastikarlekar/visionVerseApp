{
  "expo": {
    "name": "VisionVerse",
    "slug": "VisionVerse",
    "version": "1.0.0",
    "scheme": "visionverse",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "package": "com.prashasti.visionverse",
      "bundleIdentifier": "com.prashasti.visionverse"
    },
    "android": {
      "package": "com.prashasti.visionverse",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bd2c6700-60a6-444e-a4f6-83c1957cbdcd"
      }
    }
  }
}
